{
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "extends": ["airbnb-base", "eslint:recommended"],
  "env": {
    "node": true,
    "mocha": true
  },
  "plugins": [],
  "rules":{
    "eqeqeq": "off",
    "linebreak-style": 0,
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "object-curly-spacing": ["error", "never"],
    "no-console": "off",
    "import/newline-after-import": ["error", "never"]
  },
  "globals": {

  }
}